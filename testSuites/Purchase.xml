<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="Purchase"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="swarsa.TestComponents.Listeners"/>
	</listeners>
  <test thread-count="2" name="Submit Order Test">
    <classes>
      <class name="swarsa.pageobjects.SubmitOrderTest"/>
    </classes>
    </test>
    <test thread-count="2" name="Error Validations Test">
    <classes>
      <class name="swarsa.pageobjects.ErrorValidations"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
